{
  "name": "Countdown",
  "version": "1.4.0",
  "type": "module",
  "scripts": {
    "tauri": "tauri",
    "build:css": "tailwindcss -i ./src/input.css -o ./src/output.css --minify",
    "watch:css": "tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "dev": "npm run build:css && npm run tauri dev",
    "build": "npm run build:css && npm run tauri build"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1"
  }
}
